{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Represents a question/answer pair.",
	"type": "object",
	"properties": {
		"id": {
			"type": "string",
			"format": "utc-millisec",
			"description": "Unique project id (PK)."
		},
		"sequence": {
			"type": "string",
			"format": "utc-millisec",
			"description": "Sequence in the survey."
		},
		"question": {
			"type": "string",
			"description": "Question content."
		},
		"answer": {
			"type": "string",
			"description": "Answer to the question."
		}
	},
	"required": [
		"id",
		"name",
		"question"
	]
}
